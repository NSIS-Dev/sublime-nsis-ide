                { "trigger": "${SectionIsSectionGroupEnd}", "contents": "\\${SectionIsSectionGroupEnd} \\${${1}}" },
                { "trigger": "${SectionIsSelected}", "contents": "\\${SectionIsSelected} \\${${1}}" },
                { "trigger": "${Select}", "contents": "\\${Select} \\$${1:var}\n\t${4}\n\\${EndSelect}" },
                { "trigger": "${Silent}", "contents": "\\${Silent}" },
                { "trigger": "${Switch}", "contents": "\\${Switch} \\$${1:var}\n\t${4}\n\\${EndSwitch}" },
                { "trigger": "${Unless}", "contents": "\\${Select} \\$${1:var}\n\t${4}\n\\${EndUnless}" },
                { "trigger": "${While}", "contents": "\\${While} ${1:condition}\n\t${4}\n\\${EndWhile}" },

