                { "trigger": "${IfThenNot}", "contents": "\\${IfThenNot} ${1:condition} \\${|} ${4:command} \\${|}" },
                { "trigger": "${Loop}", "contents": "\\${Loop}" },
                { "trigger": "${Next}", "contents": "\\${Next}" },
                { "trigger": "${OrIfNot}", "contents": "\\${OrIfNot} ${1:condition}" },
                { "trigger": "${RebootFlag}", "contents": "\\${RebootFlag}" },
                { "trigger": "${SectionIsBold}", "contents": "\\${SectionIsBold} \\${${1}}" },
                { "trigger": "${SectionIsExpanded}", "contents": "\\${SectionIsExpanded} \\${${1}}" },
                { "trigger": "${SectionIsPartiallySelected}", "contents": "\\${SectionIsPartiallySelected} \\${${1}}" },
